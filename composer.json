{
  "name": "sbamtr/laravel-auto-hard-deleter",
  "keywords": [
    "laravel",
    "lumen",
    "softdelete",
    "database"
  ],
  "type": "library",
  "require": {
    "php": ">=7.1",
    "illuminate/console": "^5.5|^6|^7",
    "illuminate/database": "^5.5|^6|^7",
    "illuminate/support": "^5.5|^6|^7"
  },
  "require-dev": {
    "orchestra/testbench": "^3|^4|^5",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "description": "Laravel Auto Hard Deleter",
  "license": "MIT",
  "authors": [
    {
      "name": "Siavash Bamshadnia",
      "email": "sbamtr@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "sbamtr\\LaravelAutoHardDeleter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "sbamtr\\LaravelAutoHardDeleter\\Tests\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "sbamtr\\LaravelAutoHardDeleter\\AutoHardDeleteServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit -c phpunit.xml --testdox --verbose"
  }
}
